{
    "name": "native",
    "version": "1.0.0",
    "scripts": {
        "start": "expo start --dev-client",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "reinstall": "rm -rf node_modules && yarn cache clean && yarn install",
        "pull-reinstall": "git stash && git pull --rebase && rm -rf node_modules && yarn cache clean && yarn install && git stash pop",
        "build-dev": "rm -f dev.apk && eas build -p android --profile development --local --output dev.apk --clear-cache",
        "build-stg": "rm -f stg.apk && eas build -p android --profile preview --local --output stg.apk --clear-cache",
        "build-prod": "rm -f prod.apk && eas build -p android --profile production --local --output prod.apk --clear-cache"
    },
